{
  "server": false,
  "datacenter": "dc1",
  "domain": "cluster.local",
  "client_addr": "127.0.0.1",
  "log_level": "INFO",
  "ca_file": "/etc/tls/ca.pem",
  "cert_file": "/etc/tls/vault.pem",
  "key_file": "/etc/tls/vault-key.pem",
  "verify_incoming": true,
  "verify_outgoing": true,
  "verify_server_hostname": true,
  "ports": {
    "https": 8443
